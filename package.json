{
  "name": "moox.fr",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "postinstall": "cd node_modules/react-native-web/packages/react-native-web && yarn install && yarn build && cp -r . ../../",
    "prepare": "patch-package && husky && npm run svg && npm run markdown",
    "dev": "next dev --turbopack -p 1337",
    "build": "npm run prepare && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,json,ts,tsx,css,md}\" \"!package.json\"",
    "clean": "npm run svg:clean",
    "svg:clean": "rm -rf  src/svgs/components",
    "svg:web": "react-from-svg svgs src/svgs/components --with-native-for-typescript",
    "svg": "npm run svg:clean && npm run svg:web",
    "markdown": "markdown-to-json \"content/**/*.md\"",
    "test": "npm run lint && tsc --noEmit && npm run build"
  },
  "overrides": {
    "react-native": {
      "react": "$react"
    },
    "react-native-web": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-native-gesture-handler": {
      "react": "$react"
    },
    "react-native-safe-area-context": {
      "react": "$react"
    },
    "@types/react": "$@types/react",
    "@types/react-dom": "$@types/react-dom"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-clipboard/clipboard": "^1.16.1",
    "@socialgouv/matomo-next": "^1.9.2",
    "@tsconfig/next": "^2.0.3",
    "@types/react-native-web": "^0.19.0",
    "@types/tinycolor2": "^1.4.6",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.1.0",
    "date-fns": "^4.1.0",
    "next": "15.2.2",
    "react": "next",
    "react-dom": "next",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-reanimated": "^4.0.0-beta.3",
    "react-native-safe-area-context": "^4.14.0",
    "react-native-svg": "^15.10.1",
    "react-native-web": "https://github.com/necolas/react-native-web.git#fcbe2d1e9225282671e39f9f639e2cb04c7e1e65",
    "react-native-worklets": "^0.1.0",
    "tinycolor2": "^1.6.0",
    "ts-pattern": "^5.5.0"
  },
  "devDependencies": {
    "@moox/markdown-to-json": "^1.0.0",
    "@tsconfig/react-native": "^3.0.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "eslint": "^8",
    "eslint-config-next": "15.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "patch-package": "^8.0.0",
    "prettier": "^3.3.3",
    "react-from-svg": "^6.0.0",
    "react-native": "^0.78",
    "string-replace-loader": "^3.1.0",
    "typescript": "^5.8"
  }
}
